    document.getElementById =3D function() { return null; }

function initializeMenu(menuId, actuatorId) {
    var menu =3D document.getElementById(menuId);
    var actuator =3D document.getElementById(actuatorId);

    if (menu =3D=3D null || actuator =3D=3D null) return;

    // if (window.opera) return; // Opera 7 works well, so commenting this

    actuator.parentNode.style.backgroundImage =3D "url(modules/taxonomy_dht=
ml/plus.gif)";
    actuator.onclick =3D function() {
        var display =3D menu.style.display;
        this.parentNode.style.backgroundImage =3D
            (display =3D=3D "block") ? "url(modules/taxonomy_dhtml/plus.gif=
)" : "url(modules/taxonomy_dhtml/minus.gif)";
        menu.style.display =3D (display =3D=3D "block") ? "none" : "block";

        return false;
    }
}




