/*
Author: mg12
Update: 2008/05/05
Author URI: http://www.neoease.com/
*/
(function() {

function reply(authorId, commentId, commentBox) {
	var author =3D MGJS.$(authorId).innerHTML;
	var insertStr =3D '<a href=3D"#' + commentId + '">@' + author.replace(/\t|=
\n|\r\n/g, "") + ' </a> \n';

	appendReply(insertStr, commentBox);
}

function quote(authorId, commentId, commentBodyId, commentBox) {
	var author =3D MGJS.$(authorId).innerHTML;
	var comment =3D MGJS.$(commentBodyId).innerHTML;

	var insertStr =3D '<blockquote cite=3D"#' + commentBodyId + '">';
	insertStr +=3D '\n<strong><a href=3D"#' + commentId + '">' + author.replac=
e(/\t|\n|\r\n/g, "") + '</a> :</strong>';
	insertStr +=3D comment.replace(/\t/g, "");
	insertStr +=3D '</blockquote>\n';

	insertQuote(insertStr, commentBox);
}

function appendReply(insertStr, commentBox) {
	if(MGJS.$(commentBox) && MGJS.$(commentBox).type =3D=3D 'textarea') {
		field =3D MGJS.$(commentBox);

	} else {
		alert("The comment box does not exist!");
		return false;
	}

	if (field.value.indexOf(insertStr) > -1) {
		alert("You've already appended this reply!");
		return false;
	}

	if (field.value.replace(/\s|\t|\n/g, "") =3D=3D '') {
		field.value =3D insertStr;
	} else {
		field.value =3D field.value.replace(/[\n]*$/g, "") + '\n\n' + insertStr;
	}
	field.focus();
}

function insertQuote(insertStr, commentBox) {
	if(MGJS.$(commentBox) && MGJS.$(commentBox).type =3D=3D 'textarea') {
		field =3D MGJS.$(commentBox);

	} else {
		alert("The comment box does not exist!");
		return false;
	}

	if(document.selection) {
		field.focus();
		sel =3D document.selection.createRange();
		sel.text =3D insertStr;
		field.focus();

	} else if (field.selectionStart || field.selectionStart =3D=3D '0') {
		var startPos =3D field.selectionStart;
		var endPos =3D field.selectionEnd;
		var cursorPos =3D startPos;
		field.value =3D field.value.substring(0, startPos)
					+ insertStr
					+ field.value.substring(endPos, field.value.length);
		cursorPos +=3D insertStr.length;
		field.focus();
		field.selectionStart =3D cursorPos;
		field.selectionEnd =3D cursorPos;

	} else {
		field.value +=3D insertStr;
		field.focus();
	}
}

window['MGJS_CMT'] =3D {};
window['MGJS_CMT']['reply'] =3D reply;
window['MGJS_CMT']['quote'] =3D quote;

})();
